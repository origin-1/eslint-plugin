{
  "name": "@fasttime/eslint-plugin",
  "version": "0.6.1",
  "description": "fasttime ESLint plugin",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin"
  ],
  "homepage": "https://github.com/fasttime/eslint-plugin",
  "bugs": "https://github.com/fasttime/eslint-plugin/issues",
  "license": "ISC",
  "author": "Francesco Trotta <ft@fasttime.org> (https://github.com/fasttime)",
  "files": [
    "lib",
    "ts-parser.js"
  ],
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fasttime/eslint-plugin.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "coverage": "c8 --reporter=html --reporter=text-summary npm test",
    "test": "mocha --check-leaks test/{**,*/*}/*.js"
  },
  "dependencies": {
    "eslint-utils": "3"
  },
  "devDependencies": {
    "@fasttime/lint": "latest",
    "c8": "latest",
    "eslint": "latest",
    "eslint-plugin": "latest",
    "gulp": "latest",
    "mocha": "latest"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": ">=2.0.0",
    "eslint": ">=8.0.0",
    "tslib": ">=2.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
